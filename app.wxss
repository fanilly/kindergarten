/*order-list*/

.order-list {
  width: 100%;
  height: 195rpx;
  display: flex;
  margin-top: 15rpx;
  background-color: #fff;
  box-shadow: 0 0 8px rgba(153, 153, 153, .6);
  overflow: hidden;
}

.order-list .lside {
  flex: 1;
}

.order-list .lside .header {
  display: flex;
  font-size: 30rpx;
  color: #555555;
  height: 38rpx;
  padding: 15rpx;
}

.order-list .lside .header view:nth-child(1) {
  flex: 1;
  text-align: right;
  padding: 0 15rpx;
}

.order-list .lside .header view:nth-child(2) {
  width: 80rpx;
}

.order-list .lside .header view:nth-child(3) {
  padding: 0 15rpx;
  flex: 1;
}

.order-list .lside .flags {
  display: flex;
  color: #888;
  font-size: 24rpx;
  padding: 4rpx 15rpx 10rpx;
}

.order-list .lside .flags view {
  flex: 1;
  display: flex;
  align-items: center;
}

.order-list .lside .flags view text {
  padding-left: 8rpx;
}

.order-list .lside .address {
  padding: 0 15rpx;
  display: flex;
  align-items: center;
  font-size: 24rpx;
  color: #888;
}

.order-list .lside .address view:first-child {
  width: 36rpx;
}

.order-list .lside .address view:last-child {
  padding-left: 12rpx;
  line-height: 32rpx;
  flex: 1;
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.order-list .rside {
  width: 160rpx;
  position: relative;
}

.parallelogram {
  background-color: #fd6a70;
  width: 200rpx;
  height: 195rpx;
  transform: skewX(-15deg) translateX(26rpx);
  box-shadow: 0 0 8px rgba(153, 153, 153, .6);
}

.order-list .rside view:last-child {
  width: 50rpx;
  position: absolute;
  top: 0;
  right: 0;
  font-size: 36rpx;
  color: #fff;
  line-height: 56rpx;
  padding: 44rpx 28rpx 44rpx 0;
}


/*内容页样式*/

.c-header {
  background-color: #fff;
}

.c-title {
  font-size: 32rpx;
  color: #546c84;
  text-align: center;
  vertical-align: middle;
  border-bottom: 1px solid #ff5b62;
  width: 66%;
  margin: 0 auto;
  height: 88rpx;
  line-height: 88rpx;
  position: relative;
}

.triangle {
  width: 0;
  height: 0;
  border: 15rpx solid #ff5b62;
  border-right-color: transparent;
  border-left-color: transparent;
  border-bottom-color: transparent;
}

.c-title .triangle {
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
  bottom: -30rpx;
}

.c-title text {
  font-size: 36rpx;
  color: #ff5b62;
}

.c-title text.f28 {
  font-size: 30rpx;
  color: #666;
}

.text-block {
  background-color: #fff;
  margin-top: 15rpx;
}

.tb-title {
  display: flex;
  padding: 12rpx 15rpx 6rpx;
  height: 50rpx;
  line-height: 50rpx;
  border-bottom: 1px solid #e2e2e2;
}

.tb-title>view:nth-child(1) {
  width: 44rpx;
}

.tb-title>view:nth-child(2) {
  width: 180rpx;
  font-size: 28rpx;
  color: #555555;
}

.tb-title>view:nth-child(3) {
  flex: 1;
  text-align: right;
  font-size: 24rpx;
  color: #ff5b62;
}

.tb-content {
  font-size: 24rpx;
  color: #666;
  text-indent: 2em;
  padding: 15rpx;
  line-height: 40rpx;
  text-align: justify;
}

.tb-c-list {
  padding: 15rpx;
}


/*抢单*/

.tb-btns {
  width: 100%;
  height: 180rpx;
  background-color: #fff;
  display: flex;
}

.tb-btns .tb-btns-item {
  flex: 1;
  text-align: center;
  position: relative;
}

.tb-btns .tb-btns-item view:first-child {
  padding-top: 42rpx;
}

.tb-btns .tb-btns-item view:nth-child(2) {
  padding-top: 15rpx;
}

.tb-btns .tb-btns-item image {
  position: absolute;
  right: 0;
  top: 0;
}

.c-order {
  padding: 15rpx;
  margin-top: 10rpx;
  position: relative;
}

.c-order-item {
  display: flex;
  line-height: 40rpx;
  padding-top: 15rpx;
}

.c-order-item>view:nth-child(1) {
  width: 46rpx;
}

.c-order-item>view:nth-child(2) {
  width: 150rpx;
}

.c-order-item>view:nth-child(3) {
  flex: 1;
}


/*信息认证表单*/

.cer-item-wapper {
  height: 40rpx;
  padding: 30rpx 15rpx;
  border-bottom: 1px solid #f7f7f7;
  display: flex;
}

.cer-icon-wapper {
  width: 55rpx;
}

.cer-icon-wapper image {
  width: 40rpx;
  height: 40rpx;
}

.cer-title-wapper {
  font-size: 32rpx;
  color: #353535;
  line-height: 42rpx;
  padding-right: 15rpx;
}

.cer-input-wapper {
  flex: 1;
  font-size: 32rpx;
  color: #353535;
  line-height: 42rpx;
}

.cer-input-wapper .picker-content text {
  padding-right: 20rpx;
}


/*文本域*/

.cer-textarea-title {
  display: flex;
  height: 40rpx;
  font-size: 32rpx;
  color: #555;
  line-height: 42rpx;
  padding: 20rpx 15rpx;
}

.cer-textarea-title view:first-child {
  width: 55rpx;
}

.cer-textarea-title view:first-child image {
  width: 40rpx;
  height: 40rpx;
}

.cer-textarea-content {
  height: 300rpx;
  background-color: #f7f7f7;
  font-size: 32rpx;
  color: #353535;
  position: relative;
}

.cer-textarea-content textarea {
  width: 100%;
  padding: 15rpx;
  box-sizing: border-box;
  height: 300rpx;
}

.count-num-wapper {
  position: absolute;
  right: 15rpx;
  bottom: 15rpx;
  font-size: 28rpx;
  color: #b3c0c9;
}

.public-subbtn {
  font-size: 32rpx;
  text-align: center;
  width: 96%;
  margin: 50rpx auto;
  height: 80rpx;
  line-height: 80rpx;
  color: #fff;
  background-image: linear-gradient(to right, #0eaf9c, #00ad99);
  border-radius: 10rpx;
  box-shadow: 0 0 12px #ccc;
}


/*加载层*/

.loading-layer {
  text-align: center;
  margin-top: 320rpx;
}

.weui-loadmore {
  width: 65%;
  margin: 0 auto;
  color: #b3c0c9;
  line-height: 1.6em;
  font-size: 14px;
  text-align: center
}

.weui-loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: a 1s steps(12) infinite;
  animation: a 1s steps(12) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%
}

.weui-loadmore__tips {
  display: inline-block;
  vertical-align: middle
}

@keyframes a {
  0% {
    -webkit-transform: rotate(0deg);
    transform: rotate(0deg)
  }
  to {
    -webkit-transform: rotate(1turn);
    transform: rotate(1turn)
  }
}